{
  "name": "wgdo-node-react",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node server/build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "api:start": "node api/server.js",
    "api:start-watch": "nodemon api/server.js",
    "api:test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha api/**/*.test.js",
    "api:test-watch": "nodemon --exec \"npm run api:test\"",
    "lint": "esw webpack.*.js src tools --color"
  },
  "author": "",
  "license": "ISC",
